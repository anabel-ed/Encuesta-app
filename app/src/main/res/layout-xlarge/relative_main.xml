<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app=""
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="@dimen/activity_horizontal_margin">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RadioButton
            android:text="Ninguna"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/radioButton10"
            android:textSize="18sp"
            android:layout_marginEnd="29dp"
            android:layout_alignBaseline="@+id/radioButton9"
            android:layout_alignBottom="@+id/radioButton9"
            android:layout_alignParentEnd="true" />

        <RadioButton
            android:text="No"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="56dp"
            android:id="@+id/radioButton12"
            android:layout_alignBaseline="@+id/radioButton11"
            android:layout_alignBottom="@+id/radioButton11"
            android:layout_toEndOf="@+id/text_sabeLeer"
            android:textSize="18sp" />

        <RadioButton
            android:text="Primaria"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/radioButton"
            android:textSize="18sp"
            android:duplicateParentState="false"
            android:checked="false"
            android:layout_below="@+id/text_educacion"
            android:layout_alignParentStart="true"
            android:layout_marginTop="24dp" />

        <RadioButton
            android:text="Postgrado"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/radioButton9"
            android:textSize="18sp"
            android:layout_alignBaseline="@+id/radioButton8"
            android:layout_alignBottom="@+id/radioButton8"
            android:layout_toStartOf="@+id/radioButton10"
            android:layout_marginEnd="55dp" />

        <RadioButton
            android:text="Universitaria"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/radioButton8"
            android:textSize="18sp"
            android:layout_alignBaseline="@+id/radioButton7"
            android:layout_alignBottom="@+id/radioButton7"
            android:layout_toStartOf="@+id/radioButton9"
            android:layout_marginEnd="51dp" />

        <RadioButton
            android:text="Secundaria"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/radioButton7"
            android:textSize="18sp"
            android:layout_marginStart="25dp"
            android:layout_alignBaseline="@+id/radioButton"
            android:layout_alignBottom="@+id/radioButton"
            android:layout_toEndOf="@+id/text_sabeLeer" />

        <RadioButton
            android:text="Sí"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/radioButton11"
            android:textSize="18sp"
            android:duplicateParentState="false"
            android:checked="false"
            android:layout_marginTop="27dp"
            android:layout_below="@+id/text_sabeLeer"
            android:layout_alignParentStart="true"
            android:layout_marginStart="13dp" />

        <TextView
            android:text="¿SABE LEER?"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/text_sabeLeer"
            android:textColor="@android:color/background_dark"
            android:textSize="18sp"
            android:layout_marginTop="50dp"
            android:layout_below="@+id/radioButton"
            android:layout_alignParentStart="true" />

        <TextView
            android:text="NIVEL DE EDUCACIÓN"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/text_educacion"
            android:textColor="@android:color/background_dark"
            android:textSize="18sp"
            android:layout_marginTop="82dp"
            android:layout_below="@+id/input_direccion"
            android:layout_alignParentStart="true" />

        <EditText
            android:layout_height="wrap_content"
            android:inputType="date"
            android:ems="10"
            android:id="@+id/input_fecha"
            android:text="Fecha de Encuesta"
            android:maxWidth="50dp"
            android:layout_width="300dp"
            android:textColor="?android:attr/colorControlNormal"
            android:layout_marginTop="84dp"
            android:layout_alignParentTop="true"
            android:layout_alignStart="@+id/input_nombreApellido" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Nombres y Apellidos"
            android:ems="10"
            android:id="@+id/input_nombreApellido"
            android:textColor="?android:attr/colorControlNormal"
            android:layout_below="@+id/input_fecha"
            android:layout_alignStart="@+id/radioButton11" />

        <EditText
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/input_cedula"
            android:text="Documento de Identidad"
            android:layout_width="300dp"
            android:textColor="?android:attr/colorControlNormal"
            android:layout_below="@+id/input_nombreApellido"
            android:layout_alignStart="@+id/input_nombreApellido" />

        <EditText
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/input_telefono1"
            android:text="Teléfono Fijo"
            android:layout_width="300dp"
            android:textColor="?android:attr/colorControlNormal"
            android:layout_below="@+id/input_cedula"
            android:layout_alignStart="@+id/input_cedula" />

        <EditText
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/input_telefono2"
            android:text="Teléfono Movil"
            android:layout_width="300dp"
            android:textColor="?android:attr/colorControlNormal"
            android:layout_marginTop="45dp"
            android:layout_below="@+id/input_cedula"
            android:layout_alignStart="@+id/input_telefono1" />

        <EditText
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Correo Electrónico"
            android:ems="10"
            android:id="@+id/input_correo"
            android:textColor="?android:attr/colorControlNormal"
            android:layout_width="300dp"
            android:layout_below="@+id/input_telefono2"
            android:layout_alignStart="@+id/input_telefono2" />

        <EditText
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Experiencia"
            android:ems="10"
            android:id="@+id/input_experiencia"
            android:textColor="?android:attr/colorControlNormal"
            android:layout_below="@+id/input_correo"
            android:layout_alignStart="@+id/input_correo" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Lugar de Habitación"
            android:ems="10"
            android:id="@+id/input_direccion"
            android:textColor="?android:attr/colorControlNormal"
            android:layout_below="@+id/input_experiencia"
            android:layout_alignStart="@+id/input_experiencia" />

    </RelativeLayout>

</RelativeLayout>